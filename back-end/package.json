{
  "name": "back-end",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.ts",
    "dev": "nodemon",
    "lint": "eslint .",
    "format": "prettier --write .",
    "generate-env": "node scripts/generate-env.ts",
    "migrations:generate": "TS_NODE_PROJECT=tsconfig.migrations.json sh -c 'typeorm-ts-node-esm migration:generate -d src/database/data-source.ts src/migrations/\"$1\"' --",
    "migrations:run": "TS_NODE_PROJECT=tsconfig.migrations.json typeorm-ts-node-esm migration:run -d src/database/data-source.ts",
    "migrations:revert": "TS_NODE_PROJECT=tsconfig.migrations.json typeorm-ts-node-esm migration:revert -d src/database/data-source.ts"
  },
  "keywords": [
    "programming",
    "education",
    "code-judge",
    "c-programming"
  ],
  "author": "",
  "license": "ISC",
  "description": "Backend escalable para MVP de evaluación de ejercicios de programación en C",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.26",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.1",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
